<script type="text/javascript">
    function CheckPayBankinfo(formId, formPerfix) {        
        var account = $("#" + formPerfix + "_bank_payaccount").val();
        var account2 = $("#" + formPerfix + "_bank_payaccount2").val();
        var amount = $("#" + formPerfix + "_bank_payamount").val();
        if (account == "") {
            alert("请填写充值用户名。");
            return;
        }
        else {
            if (account != account2) {
                alert("两次输入的账号不一致！");
                return;
            }
        }
        var re = /^[1-9]+[0-9]*$/;
        if (amount == "") {
            alert("请填写充值金额。");
            return;
        }
        else {
            if (!re.test(amount)) {
                alert("充值金额应为大于0的整数！");
                return;
            }
        }
        $("#" + formId).submit();
    }
</script>



