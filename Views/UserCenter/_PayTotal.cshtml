                <script>
                    var zzStatus = 0;
                    function CheckPayBankinfo(formId, formPerfix) {
                        var account = $("#" + formPerfix + "_bank_payaccount").val();
                        var account2 = $("#" + formPerfix + "_bank_payaccount2").val();
                        var amount = $("#" + formPerfix + "_bank_payamount").val();
                        if (account == "") {
                            alert("请填写充值用户名。");
                            return;
                        }
                        else {
                            if (account != account2) {
                                alert("两次输入的账号不一致！");
                                return;
                            }
                        }
                        var re = /^[1-9]+[0-9]*$/;
                        if (amount == "") {
                            alert("请填写充值金额。");
                            return;
                        }
                        else {
                            if (!re.test(amount)) {
                                alert("充值金额应为大于0的整数！");
                                return false;
                            }
                        }
                        $("#" + formId).submit();
                    }
                    function CheckPayAlipay(formId, formPerfix) {
                        var account = $("#" + formPerfix + "_bank_payaccount").val();
                        var account2 = $("#" + formPerfix + "_bank_payaccount2").val();                        
                        if (account == "") {
                            alert("请填写充值用户名。");
                            return;
                        }
                        else {
                            if (account != account2) {
                                alert("两次输入的账号不一致！");
                                return;
                            }
                        }                        
                        $("#" + formId).submit();
                    }
                    function CheckPayment(button)
                    {
                        var cname  = $("#manual_cardholder");
                        var pdata = $("#manual_transfer_date");
                        var amount = $("#manual_order-amount");
                        var fs = $("#manual_transfer-mode");
                        var fstext = $("#manual_other-mode");
                        var caddress = $("#recharge_transfer_address");
                        if(isNaN(amount.val()) || parseInt(amount.val()) < @Model.MinPayAmount || amount.val() == "")
                        {
                            alert("请输入一个大于或等于 @Model.MinPayAmount 的数字。");
                            return false;
                        }
                        else if(fs.val() == "0" && fstext.val()=="")
                        {
                            alert("请填写汇款方式。");
                            return false;
                        }
                        else if(cname.val() == "")
                        {
                            alert("请填写持卡人姓名。");
                            return false;
                        }
                        else if(pdata.val() == "")
                        {
                            alert("请填写汇款时间。");
                            return false;
                        }
                        else if(caddress.val() == "")
                        {
                            alert("请填写汇款地点。");
                            return false;
                        }
                        if (zzStatus == 1) {
                            alert('正在提交信息中，请稍后...');
                            return;
                        }
                        else{
                            zzStatus=1;
                            $(button).trigger("submit");
                        }
                    }
                </script>
